    "lint": "standard",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ]
    }
  },
  "dependencies": {
    "local-links": "^1.4.0",
    "vdom-serialized-patch": "^1.0.2",
