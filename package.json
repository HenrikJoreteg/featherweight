{
  "name": "featherweight",
  "description": "featherweight application pattern",
  "version": "3.0.0",
  "author": "Henrik Joreteg <henrik@joreteg.com> (https://joreteg.com)",
  "bugs": {
    "url": "https://github.com/HenrikJoreteg/featherweight/issues"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "node-babel": "^0.1.2",
    "nodemon": "^1.8.1",
    "npm-watch": "^0.1.1",
    "standard": "^5.4.1",
    "tape": "^4.3.0"
  },
  "files": [
    "src",
    "lib"
  ],
  "homepage": "https://github.com/HenrikJoreteg/featherweight#readme",
  "jsnext:main": "src/index.js",
  "keywords": [
    "application",
    "lightweight"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HenrikJoreteg/featherweight.git"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
